%tr.dataRow.even.first.last
  %td= "Application - #{filled_form.id}"
  %td= filled_form.company_name
  %td
    %span= filled_form.PUBLISHED? ?  "Submitted" : "Draft"
  %td.action-td
    .row
      .col-lg-7
        - unless filled_form.PUBLISHED?
          = link_to 'Continue Application', edit_insurance_service_path(filled_form.id), {class: 'btn btn-sm btn-primary', data: {turbolinks: "false"}}.merge!(filled_form.PUBLISHED? ? {disabled: "disabled"} : {})
      .col-lg-5.download
        = link_to download_pdf_insurance_service_path(filled_form), {target: '_blank', title: 'Download PDF'} do
          %i.fa.fa-arrow-circle-o-down{"aria-hidden" => "true"}
        = link_to "#{form_xml_content_insurance_service_path(filled_form.id)}.xml", {target: '_blank', title: 'View form fields XML content'} do
          %i.fa.fa-file-excel-o{"aria-hidden" => "true"}
